{
  "name": "power-hour-server",
  "description": "The Power Hour backend.",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "format": "npx prettier --write .",
    "compile": "tsc",
    "build": "npm-run-all clean lint compile",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "mocha -r ts-node/register tests/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BTx123/power-hour-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BTx123/power-hour-server/issues"
  },
  "homepage": "https://github.com/BTx123/power-hour-server#readme",
  "dependencies": {
    "cookie-session": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "socket.io": "^4.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "@types/socket.io-client": "^3.0.0",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.1.2",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
